// Ctrl + V --> 
$ git clone https:

// sprawdzenie czy jest zainstalowane
$ ls 
$ ls -la

cd (nazwa repozytorium) - wejście do plilu

// Dod. plik
touch test.txt

// Dod. do pliku
echo "Nowy plik na serwerze" > test.txt 

// Otwiera plik
cat test.txt

dir

clear

git status

git add

git commit -m "dodanie nowego pliku"

// wysyła na gita
git push

history

$ git config --global user.name "KarynaPomin"
$ git config --global user.email "karinkapomin@gmail.com"

mkdir lekcja_1

// Jak dodać do gita
   1  git add *
   2  git commit -m "Dodano"
   3  git push

// dodać folder do gita 
    1  git clone
    2  git add*
    3  git status
    4  git commin -m "add new project"
    5  git status
    6  git push
    7  git status
    8  history





